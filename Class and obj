class Parent {
  constructor() {
    this.name = "Unknown";
    this.age = 0;
  }

  get parentInfo() {
    return `Name: ${this.name}, Age: ${this.age}`;
  }

  set changeName(newName) {
    this.name = newName;
  }

  set changeAge(newAge) {
    if (newAge > 0) {
      this.age = newAge;
    } else {
      console.log("Age must be positive!");
    }
  }
}

const dad = new Parent();
console.log(dad.parentInfo);

dad.changeName = "Ramesh";
dad.changeAge = 45;
console.log(dad.parentInfo);

dad.changeAge = -5;
